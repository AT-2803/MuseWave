{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/index.ts"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/public/assets/$1"
    }
  ],
  "functions": {
    "src/index.ts": {
      "maxDuration": 300
    }
  }
}